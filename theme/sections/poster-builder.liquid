body .poster-cont .poster-cont-inner .arrow-cont .arrow-round {
    position: relative;
    width: 48px;
    height: 48px;
  	transform: rotate(180deg);  
    border-radius: 100%;

    background-color: black;
    box-shadow: black 0 0 0 0;
    transition: box-shadow 0.5s, background-color 0.5s;
    animation-name: bounceCircle;
    animation-duration:1.3s;
    animation-delay: 0.5s;
    animation-iteration-count:1;
    animation-timing-function: ease-out;
}
body .poster-cont .poster-cont-inner .arrow-cont .arrow-round #arrow-cta{
  width:100%;
  height: 100%;
  border-radius: 100%;
  cursor: pointer; 
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  transform: translateY(0px);
}


body .poster-cont .poster-cont-inner .arrow-cont .arrow-round #arrow-cta svg{
  fill: white;
  stroke-width: 2px;
  stroke: black;
  width: 24px;
  height: 24px;
  margin-top: -6px;
  transform: translateY(0px);
  animation-name: bounceArrow;
  animation-delay: 0.5s;
  animation-duration:1.3s;
  animation-iteration-count:1;
  animation-timing-function: ease-out;
}

@keyframes bounceCircle {
  0% {transform: translateY(0px) rotate(180deg);}
  20% {transform: translateY(5px) rotate(180deg);}
  40% {transform: translateY(-3px) rotate(180deg);}
  60% {transform: translateY(2px) rotate(180deg);}
  80% {transform: translateY(-1px) rotate(180deg);}
  100% {transform: translateY(0px) rotate(180deg);}
}
@keyframes bounceArrow {
  0% {transform: translateY(0px);}
  20% {transform: translateY(12px);}
  40% {transform: translateY(-8px);}
  60% {transform: translateY(5px);}
  80% {transform: translateY(-3px);}
  100% {transform: translateY(0px);}
}


