{% if
  template.name == 'blog'
  or template.name == 'collection'
  or template.name == 'list-collections'
  or template.name == 'search'
%}
  {%- assign hide_footer = true -%}
{% endif %}

<!doctype html>
<!--[if IE 9]> <html class="ie9 supports-no-js{% if hide_footer %} site-footer--hidden{% endif %}" lang="{{ shop.locale }}"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="supports-no-js {% if hide_footer %} site-footer--hidden{% endif %}" lang="{{ shop.locale }}"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <!--[if IE 9]><meta http-equiv="X-UA-Compatible" content="IE=9,chrome=1"><![endif]-->
  <!--[if (gt IE 9)|!(IE)]><!--><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><!--<![endif]-->
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_primary }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <!-- Google Tag Manager -->
    <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
    new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
    j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
    'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
    })(window,document,'script','dataLayer','GTM-MR6R8VQ');</script>
  <!-- End Google Tag Manager -->
  {% if settings.favicon != blank %}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {% endif %}
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css"/>
  <title>
    {{ page_title }}{% if current_tags %}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}">
  {% endif %}
  {% if template contains 'product' %}
    <meta name="robots" content="noindex">
  {% endif %}

  {% include 'social-meta-tags' %}

  {{ content_for_header }}

  {% include 'critical-css' %}
  {% include 'load-css' %}
  
  

  <script>
    var theme = {
      stylesheet: "{{ 'theme.scss.css' | asset_url }}",
      strings: {
        addToCart: {{ 'products.product.add_to_cart' | t | json }},
        soldOut: {{ 'products.product.sold_out' | t | json }},
        unavailable: {{ 'products.product.unavailable' | t | json }},
        addItemSuccess: {{ 'products.product.item_added_to_cart' | t | json }},
        cartError: {{ 'cart.general.cart_error' | t | json }},
        cartQuantityError: {{ 'cart.general.cart_quantity_error_html' | t: quantity: "[quantity]", title: "[title]" | json }},
        newsletter: {
          blankError: {{ 'general.newsletter_form.blank_error' | t | json }},
          invalidError: {{ 'general.newsletter_form.invalid_error' | t | json }}
        },
        map: {
          zeroResults: {{ 'sections.map.address_no_results' | t | json }},
          overQueryLimit: {{ 'sections.map.address_query_limit_html' | t | json }},
          authError: {{ 'sections.map.auth_error_html' | t | json }},
          addressError: {{ 'sections.map.address_error' | t | json }}
        }
      },
      isOnline: true,
      moneyFormat: {{ shop.money_format | json }},
      mediaQuerySmall: 750,
      mediaQueryMedium: 990
    };

    {% if template.directory == 'customers' %}
      theme.countryOptionTags = {{ all_country_option_tags | json}};
    {% endif %}

    document.documentElement.className = document.documentElement.className.replace('supports-no-js', '');
  </script>

  <!--[if lte IE 9]><script src="{{ 'polyfills.js' | asset_url }}"></script><![endif]-->

  <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'lazysizes.min.js' | asset_url }}" async="async"></script><!--<![endif]-->
  <!--[if lte IE 9]><script src="{{ 'lazysizes.min.js' | asset_url }}"></script><![endif]-->

  <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'theme.min.js' | asset_url }}" defer="defer"></script><!--<![endif]-->
  <!--[if lte IE 9]><script src="{{ 'theme.min.js' | asset_url }}"></script><![endif]-->

  <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'custom.js' | asset_url }}" defer="defer"></script><!--<![endif]-->
  <!--[if lte IE 9]><script src="{{ 'custom.js' | asset_url }}"></script><![endif]-->
 
  <style>
  	.site-header .site-header__wrapper .page-width {
      max-width: none;
      padding: 0 21px;
    }
    @media (max-width: 767px){
      .site-header .site-header__wrapper .page-width .site-header__logo-wrapper .site-header__logo-image {
          height: 38px;
      }
    }
    .site-header__cart svg.icon .cls-cart {
      fill: none;
      stroke: #000;
      stroke-miterlimit: 10;
      stroke-width: 2px;
    }
  </style>
  {% include 'shogun-head' %}
  {% include 'under-construction' %}

  {% include 'bold-common' %}

  {% include 'hide-amazon-button' %}{% include 'hide-apple-button' %}{% include 'hide-google-button' %}{% include 'hide-paypal-button' %}<script src="//staticxx.s3.amazonaws.com/aio_stats_lib_v1.min.js?v=1.0"></script>
</head>

<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | split: '.' | first | handle }} data-animations">
  <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-MR6R8VQ" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->
  {% if page_title contains 'Cart' %}
  	<script>window.location.href = '/pages/create-artwork?cartopen=true'</script>
  {% endif %}
  <a class="in-page-link visually-hidden skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>

  {% section 'header' %}

  <div class="main-content" data-drawer-push>
    <div class="main-content__wrapper">

      <main class="content-for-layout" id="MainContent" role="main">
        {{ content_for_layout }}
      </main>
	  
      {%if page_title contains 'Modern art for pets and their humans' or page_title contains 'Create Artwork'%}
      	{% section 'insta-scroller-section' %}
      {%endif%}
      {%if page_title contains 'Modern art for pets and their humans' %}
      <script defer="defer">
        $(function(){
          function init(){
          	fbq('trackCustom', 'Visit - Landing page');
            $('#Slideshow-slideshow').on('click', 'a', function(){
              fbq('trackCustom', 'Click - Landing page, Create Artwork-button, slideshow');
            })
            $('.summary-section').on('click', 'a', function(){
              fbq('trackCustom', 'Click - Landing page, Create Artwork-button, summary');
            })
          }
          
          var checkForFbq = setInterval(function(){
            if(fbq){
            	init();
              	clearInterval(checkForFbq);
            }
          }, 100)
          
        })
      </script>
      {%endif%}
      {%if page_title contains 'Create Artwork' %}
      <script defer="defer">
        $(function(){
          fbq('trackCustom', 'Visit - Create Artwork page')
          $('#shopify-section-poster-builder').on('click', '.add-to-cart', function(){
          	fbq('trackCustom', 'Click - Create Artwork page, Add-to-Cart button');
          })
        })
      </script>
      {%endif%}
      <script defer="defer">
        $(function(){
          $('body').on('click', '.cart-drawer__checkout', function(){
          	fbq('trackCustom', 'Click - Cart drawer, Checkout button');
          })
        })
      </script>
      
      
      {% section 'footer' %}
    </div>
  </div>

  {% include 'cart-drawer' %}

  {% include 'cookie-custom' %}

<!-- BEEKETINGSCRIPT CODE START -->{% include 'bk-tracking' %}<!-- BEEKETINGSCRIPT CODE END -->

 {% include 'bold-currency-converter' %}

<input type="hidden" id="656565115116111114101" data-customer-id="{{ customer.id }}" data-customer-email="{{ customer.email }}" data-customer-name="{{ customer.first_name }} {{ customer.last_name }}"/></body>
</html>
